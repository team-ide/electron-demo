{
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "electron/*": [
        "./electron/*"
      ]
    },
    "target": "ES6",
    "module": "ES6",
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "include": [
    "src",
    "electron"
  ],
  "implicitlyTypedLibraries": [
    "node"
  ],
}